  <code>randomize();
var f = irandom(16);
var s = irandom(f);
var fd = irandom(16);
var sd = irandom(fd);
show_debug_message(string(f - s) + " " + string(fd - sd))

global.particles = part_system_create();
part_system_depth(global.particles, -1000000);

global.p_card_boom = part_type_create();
part_type_sprite(global.p_card_boom, spr_combo_boom, true, true, false);
part_type_blend(global.p_card_boom, true);
part_type_life(global.p_card_boom, 25, 25);

global.p_coin_flip = part_type_create();
global.p_coin_pick = part_type_create();
global.p_coin_move = part_type_create();
part_type_sprite(global.p_coin_flip, spr_coin, true, false, false);
part_type_life(global.p_coin_flip, 60, 60);
part_type_death(global.p_coin_flip, 1, global.p_coin_pick);

part_type_life(global.p_coin_pick, 30, 30);
part_type_death(global.p_coin_pick, 1, global.p_coin_move);

part_type_life(global.p_coin_move, 10, 10);
part_type_alpha2(global.p_coin_move, 1, 0)
part_type_speed(global.p_coin_move, 14, 14, 0, 0)

global.card_deck = 0;
for (var i = 0; i &lt; 100; i++) {
    global.card_deck[i] = ds_list_create();
}

global.quickplay = true;

if (file_exists("cards.deck")) {
    var file = file_text_open_read("cards.deck");
    for (var i = 0; i &lt; 100; i++) {
        ds_list_read(global.card_deck[i], file_text_read_string(file));
        file_text_readln(file);
        
        for (var j = 0; j &lt; ds_list_size(global.card_deck[i]); j++) {
            var card = ds_list_find_value(global.card_deck[i], j);
            show_debug_message(debug_card_string(card[0], card[1], card[2], card[3], card[4], card[5], false));
        }
        
    }
    file_text_close(file);
    
} else {
    repeat (10) {
        var num = irandom(12);
        ds_list_add(global.card_deck[num], generate_new_card(num));
    }
    
    var file = file_text_open_write("cards.deck");
    for (var i = 0; i &lt; 100; i++) {
        file_text_write_string(file, ds_list_write(global.card_deck[i]));
        file_text_writeln(file);
    }
    file_text_close(file);
}

enum connstatus {
    none = 0,
    host = 1,
    client = 2,
    waiting = 3,
    error = 4
}

audio_play_sound(mus_quadmist, 0, true);</code>
